<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro">

    <material name="green">
        <!-- rgba = red, green, blue, alpha(transparency) -->
        <color rgba="0 0.5 0 1.0"/>
    </material>

    <material name="blue">
        <!-- rgba = red, green, blue, alpha(transparency) -->
        <color rgba="0 0 0.5 1.0"/>
    </material>

    <material name="gray">
        <color rgba="0.5 0.5 0.5 1.0"/>
    </material>

</robot>
